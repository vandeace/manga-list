ssword,\n    };\n    dispatch(Login(payload));\n  };\n\n  useEffect(() => {\n    if (isAuth) {\n      toggle();\n    }\n    // eslint-disable-next-line\n  }, [isAuth]);\n\n  const toggle = () => {\n    props.setModal(!props.setModal);\n  };\n\n  const clearError = () => {\n    setError({\n      error: false,\n      message: \"\",\n    });\n  };\n\n  return (\n    <div>\n      <Modal\n        isOpen={props.modal}\n        style={customStyles}\n        contentLabel=\"Modal Login\"\n        ariaHideApp={false}\n      >\n        {loading && <Loader />}\n        <div className=\"\" style={{ minWidth: 400 }}>\n          <div className=\"block\">\n            <FontAwesomeIcon\n              icon={faTimes}\n              onClick={toggle}\n              className=\"cursor-pointer\"\n            />\n          </div>\n          <h2 className=\"text-4xl font-mono font-extrabold text-blue-primary text-center my-3\">\n            Login\n          </h2>\n\n          <form onSubmit={handleSubmit(onSubmit)}>\n            <div className=\"flex flex-col\">\n              <input\n                type=\"text\"\n                placeholder=\"Email\"\n                name=\"email\"\n                onChange={() => clearError()}\n                className=\"w-full border-gray-light border my-2 p-2 pl-4 rounded-3xl focus:outline-none shadow appearance-none \"\n                ref={register({ required: true, pattern: /^\\S+@\\S+$/i })}\n              />\n              <input\n                type=\"password\"\n                placeholder=\"Password\"\n                onChange={() => clearError()}\n                name=\"password\"\n                ref={register({ required: true, min: 8 })}\n                className=\"w-full border-gray-light border my-2 p-2 pl-4 rounded-3xl focus:outline-none shadow appearance-none \"\n              />\n              {error.error && (\n                <p className=\"text-red text-center\">{error.message}</p>\n              )}\n              <input\n                type=\"submit\"\n                className=\"w-full my-2 p-2 rounded-3xl bg-light-blue hover: hover:bg-blue-primary cursor-pointer focus:outline-none shadow appearance-none \"\n              />\n            </div>\n          </form>\n        </div>\n      </Modal>\n    </div>\n  );\n};\n\nexport default LoginModal;\n","/Users/jafar/Documents/MyCode/pahamify/pahamify/src/components/Modals/SearchManga.js",[],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/config/api.js",[],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/components/Modals/Register.js",[],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/pages/Home.js",["93","94","95","96","97"],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/App.js",[],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/components/Modals/Update.js",[],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/components/Loader/Loader.js",[],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/components/Modals/Answer.js",[],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/components/_redux/store/index.js",[],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/components/_redux/actions/collection.js",[],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/components/_redux/reducers/collection.js",[],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/components/_redux/store/types.js",[],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/components/_redux/store/middlewares.js",[],"/Users/jafar/Documents/MyCode/pahamify/pahamify/src/components/_redux/actions/auth.js",["98"],"import { API } from \"config/api\";\nimport { LOGIN } from \"components/_redux/store/types\";\nimport axios from \"axios\";\n\n// export const Login = (payload = {}) => async (dispatch) => {\n//   console.log(\"login\");\n//   const res = API.post(\"/login\", payload);\n//   if (res?.status === 200) {\n//     dispatch(getCollection());\n//   }\n// };\n\nexport const Login = (payload) => {\n  return {\n    type: LOGIN,\n    payload: axios({